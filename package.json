{
	"name": "portfolio",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "astro dev --host",
		"start": "astro preview",
		"build": "astro check && astro build && pnpm run postbuild",
		"preview": "astro preview",
		"astro": "astro",
		"postbuild": "pnpm run pagefind",
		"clean": "rm -rf ./.astro ./dist ./.vercel ./public/_pagefind",
		"lint": "eslint . --fix",
		"pagefind": "if [ -d ./dist/client ]; then npm run pagefind:node; elif [ -d .vercel/output/static ]; then npm run pagefind:vercel; fi",
		"pagefind:node": "pagefind --site ./dist/client --output-subdir _pagefind",
		"pagefind:vercel": "pagefind --site .vercel/output/static --output-subdir _pagefind",
		"pagefind:dev": "if [ -d ./dist/client ]; then cp -rf ./dist/client/_pagefind ./public ; elif [ -d .vercel/output/static ]; then cp -rf .vercel/output/static/_pagefind ./public; fi",
		"format": "prettier --write . && prettier --check ."
	},
	"dependencies": {
		"@astrojs/check": "^0.7.0",
		"@astrojs/mdx": "^3.0.0",
		"@astrojs/node": "^8.2.5",
		"@astrojs/partytown": "^2.1.0",
		"@astrojs/rss": "^4.0.6",
		"@astrojs/sitemap": "^3.1.4",
		"@astrojs/svelte": "^5.4.0",
		"@astrojs/tailwind": "^5.1.0",
		"@astrojs/vercel": "^7.6.0",
		"@builder.io/qwik": "^1.4.5",
		"@fontsource-variable/inter": "^5.0.18",
		"@fontsource-variable/raleway": "^5.0.20",
		"@qwikdev/astro": "^0.5.7",
		"@unpic/qwik": "^0.0.44",
		"@vercel/og": "^0.6.2",
		"astro": "^4.8.4",
		"astro-breadcrumbs": "^2.3.0",
		"astro-font": "^0.0.81",
		"astro-icon": "^1.1.0",
		"astro-imagetools": "0.9.0",
		"daisyui": "^4.7.3",
		"sharp": "0.33.2",
		"svelte": "^4.2.15"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.6.1",
		"@commitlint/config-conventional": "^18.6.2",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/container-queries": "0.1.1",
		"@tailwindcss/typography": "0.5.10",
		"@types/fs-extra": "^11.0.4",
		"@types/lodash": "^4.14.202",
		"@types/node": "^20.11.25",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"astro-robots-txt": "^1.0.0",
		"astro-webmanifest": "^1.0.0",
		"autoprefixer": "^10.4.18",
		"cheerio": "1.0.0-rc.12",
		"clsx": "^2.1.0",
		"eslint": "^8.57.0",
		"eslint-config-google": "^0.14.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-astro": "^0.31.4",
		"eslint-plugin-qwik": "^1.5.1",
		"github-slugger": "^2.0.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"mdast-util-to-string": "^3.2.0",
		"node-html-to-image": "^4.0.0",
		"pagefind": "^1.1.0",
		"prettier": "^3.2.5",
		"prettier-plugin-astro": "0.13.0",
		"prettier-plugin-tailwindcss": "^0.5.12",
		"reading-time": "^1.5.0",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-format": "^5.0.0",
		"rehype-pretty-code": "^0.13.0",
		"rehype-stringify": "^10.0.0",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.0.0",
		"remark-toc": "^8.0.1",
		"tailwind-merge": "^2.2.1",
		"tailwindcss": "^3.4.1",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.4.2",
		"unified": "^11.0.4",
		"vite": "^5.1.5",
		"zod": "^3.22.5"
	}
}
