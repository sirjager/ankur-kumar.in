{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "@builder.io/qwik",
		"strictNullChecks": true,
		"resolveJsonModule": true,
		"noImplicitReturns": false,
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		}
	},
	"files": ["./.eslintrc.cjs"],
	"exclude": ["node_modules"],
	"include": [
		"src",
		"**/*.ts",
		"**/*.js",
		"**/*.tsx",
		"**/*.jsx",
		"**/*.mjs",
		"**/*.cjs"
	]
}
